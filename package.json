  "scripts": {
    "build": "wp-scripts build",
    "build:tsc": "nx run-many --target=build:tsc",
    "build:types": "nx run-many --target=build:types",
    "packages-update-deep": "nx run-many --target=packages-update",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format": "wp-scripts format",
    "format:src": "wp-scripts format ./src",
    "format:packages": "wp-scripts format ./packages/**/src",
    "format:php": "wp-env run composer run-script format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:js:src": "wp-scripts lint-js ./src",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:md:js": "wp-scripts lint-md-js",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "prelint:php": "wp-env run composer \"install --no-interaction\"",
    "lint:php": "wp-env run composer run-script lint",
    "packages-update": "wp-scripts packages-update && npx nx run-many --target=packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "start": "wp-scripts start",
    "start:hot": "wp-scripts start --hot",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:php": "npm run lint:php && npm run test:unit:php",
    "test:php:watch": "wp-env run composer run-script test:watch",
    "pretest:unit:php": "wp-env start",
    "test:unit:php": "wp-env run phpunit /var/www/html/wp-content/plugins/user-menus/vendor/bin/phpunit -c /var/www/html/wp-content/plugins/user-menus/phpunit.xml.dist --verbose",
    "translate": "wp-pot --src './classes/**/*.php' --src '!./build/**/*.*' --src '!./vendor/**/*.*' --src '!./dist/**/*.*' --write=true --dest-file 'languages/user-menus.pot' --package 'User Menus' --domain 'user-menus' --last-translator 'Code Atlantic <support@code-atlantic.com>' --team 'Code Atlantic <support@code-atlantic.com>' --bug-report 'https://code-atlantic.com/contact/'",
    "release": "composer install --no-dev --classmap-authoritative && npm run build && npm run translate && npm run plugin-zip",
    

    "build": "gulp prebuild",
    "release": "gulp release"
  },
  "babel": {},
  "workspaces": [
    "./packages/*"
  ],
  "files": [
    "dist",
    "classes",
    "assets/images",
    "assets/scripts/*.js",
    "assets/styles/*.css",
    "inc",
    "languages",
    "vendor/autoload.php",
    "vendor/composer/**/*.php",
    "vendor-prefixed",
    "readme.txt",
    "index.php",
    "index.php",
    "user-menus.php",
    "uninstall.php"
  ]
